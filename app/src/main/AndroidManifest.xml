<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.csumb.abmedina.cst438_project_personal_recipe_finder">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUpActivity" />
        <activity android:name=".Admin" />
        <activity
            android:name=".ProfileActivity"
            android:windowSoftInputMode="adjustPan" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".FridgeDashboardActivity" />
        <activity android:name=".MeatActivity" />
        <activity android:name=".GrainsActivity" />
        <activity android:name=".VegetablesActivity" />
        <activity android:name=".FruitsActivity" />
        <activity android:name=".DairyActivity" />
        <activity android:name=".OtherFoodActivity" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".ShoppingListActivity" />
        <activity android:name=".ListRecipesFoundActivity" />
        <activity android:name=".MyRecipesActivity" />
    </application>

</manifest>